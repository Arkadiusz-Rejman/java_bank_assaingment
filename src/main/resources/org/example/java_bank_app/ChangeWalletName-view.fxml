            <ImageView fitHeight="44.0" fitWidth="150.0">
               <image>
                  <Image url="@../../../IconPackage/button_icon.png" />
               </image>
            </ImageView>
            <Label fx:id="change_label" layoutX="45.0" layoutY="9.0" text="Change">
               <font>
                  <Font size="18.0" />
               </font>
            </Label>
         </children>
      </Group>
   </children>
</AnchorPane>
