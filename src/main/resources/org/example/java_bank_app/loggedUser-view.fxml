      <Button layoutX="104.0" layoutY="238.0" mnemonicParsing="false" onAction="#previousWallet" text="previous" />
      <Button layoutX="157.0" layoutY="136.0" mnemonicParsing="false" onAction="#deleteWallet" text="delete wallet" />
      <Button fx:id="refreshButton" layoutX="404.0" layoutY="181.0" mnemonicParsing="false" onAction="#refreshData" text="refresh" />
   </children>
</AnchorPane>
