         <font>
            <Font name="Avenir Next Bold" size="15.0" />
         </font>
      </Label>
      <Label fx:id="UserNameLabel" layoutX="504.0" layoutY="32.0" text="Name">
         <font>
            <Font name="Avenir Next Bold" size="15.0" />
         </font>
      </Label>
      <Button layoutX="345.0" layoutY="22.0" mnemonicParsing="false" text="Account Options" />
