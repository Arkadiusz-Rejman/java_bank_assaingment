      <Button layoutX="37.0" layoutY="76.0" mnemonicParsing="false" text="transfer money" />
      <Button layoutX="169.0" layoutY="76.0" mnemonicParsing="false" text="transaction history" />
      <Label layoutX="505.0" layoutY="14.0" />
