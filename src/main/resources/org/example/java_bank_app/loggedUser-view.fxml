      <Label layoutX="505.0" layoutY="14.0" />
      <Label layoutX="473.0" layoutY="11.0" text="Welcome back!">
         <font>
            <Font name="Big Caslon Medium" size="15.0" />
         </font>
      </Label>
      <Label fx:id="UserNameLabel" layoutX="504.0" layoutY="32.0" text="Name">
         <font>
            <Font name="Big Caslon Medium" size="15.0" />
         </font>
      </Label>
      <Button layoutX="345.0" layoutY="22.0" mnemonicParsing="false" text="Account Options" />
