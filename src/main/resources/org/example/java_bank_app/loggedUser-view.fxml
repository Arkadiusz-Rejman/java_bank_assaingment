      <Label fx:id="WalletNameLabel" layoutX="202.0" layoutY="147.0" prefHeight="30.0" prefWidth="179.0" text="Wallet Name">
         <font>
            <Font name="Big Caslon Medium" size="20.0" />
         </font>
      </Label>
      <Label fx:id="BalanceLabel" layoutX="218.0" layoutY="240.0" prefHeight="23.0" prefWidth="133.0" text="balance:">
         <font>
            <Font name="Big Caslon Medium" size="20.0" />
         </font>
      </Label>
      <Label fx:id="CurrencyLabel" layoutX="218.0" layoutY="271.0" prefHeight="23.0" prefWidth="133.0" text="currency:">
         <font>
            <Font name="Big Caslon Medium" size="20.0" />
         </font>
      </Label>
      <Button layoutX="383.0" layoutY="136.0" mnemonicParsing="false" text="Wallet Options" />
