<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.java_bank_app.ControllersPackage.LoggedUserController">
   <children>
      <ImageView fitHeight="274.0" fitWidth="292.0" layoutX="150.0" layoutY="116.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../IconPackage/wallet-flat.png" />
         </image>
      </ImageView>
      <Button layoutX="114.0" layoutY="187.0" mnemonicParsing="false" onAction="#openAddWallets" text="add wallet" />
      <Button fx:id="transfer_button" layoutX="37.0" layoutY="76.0" mnemonicParsing="false" onAction="#openTransferWindow" text="transfer money" />
